{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Webpack 脚手架",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development npx webpack-dev-server --config webpack.dev.js",
