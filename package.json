    "prod:build": "cross-env NODE_ENV=development npx webpack --config webpack.prod.js",
    "lint": "eslint --ext .js,.jsx,.vue src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.vue src"
  },
  "keywords": [],
